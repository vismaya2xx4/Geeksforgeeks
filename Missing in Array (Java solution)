class Solution {
    int missingNum(int arr[]) {
        // code here
        int sum =0;
        int n = arr.length;
        
        for ( int i =0;i<n;i++){
            sum = sum+arr[i];
    
            
        }
        int s =(n+1)*(n+2)/2;
            
            int result= s-sum;
         return result;
    }
}
